@TECHREPORT{eijkhout_15a,
  author = {Eijkhout, Victor},
  title = {Teaching MPI by Concept},
  institution = {University of Texas at Austin},
  year = {2015},
  abstract = {The Message Passing Interface (MPI) is a de facto standard for programming
	large
	
	scale parallelism, with up to millions of individual processes. Its
	dominant paradigm
	
	of Single Program Multiple Data (SPMD) programming is different from
	threaded and
	
	multicore parallelism, to an extent that students have a hard time
	making the mental
	
	transition to this model.
	
	
	We identify problems with the current way of teaching MPI, and propose
	a better way.
	
	Instead of presenting routines in a commonly accepted order, we largely
	reverse this
	
	order and motivate this by giving successive practical scenarios that
	call for the basic
	
	blocks.},
  comment = {Open for comments},
  url = {\url{https://bitbucket.org/VictorEijkhout/parallel-computing-book}}
}

